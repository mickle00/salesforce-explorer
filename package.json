{
  "name": "salesforce-explorer",
  "version": "0.1.0",
  "description": "An explorer for Salesforce, like workbench but on your desktop",
  "author": "Stan Lemon <stosh1985@gmail.com>",
  "repository": "https://github.com/stanlemon/salesforce-explorer",
  "license": "MIT",
  "keywords": [],
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --cache --color --ext .jsx,.js src",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check"
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm"
      },
      "electronWinstallerConfig": {
        "name": "salesforce_explorer"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "stosh1985@gmail.com",
        "name": "Stan Lemon"
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "salesforce_explorer"
      }
    }
  },
  "dependencies": {
    "@salesforce-ux/design-system": "^2.2.2",
    "electron-compile": "^6.4.3",
    "electron-devtools-installer": "^2.1.0",
    "electron-squirrel-startup": "^1.0.0",
    "jsforce": "^1.7.1",
    "keytar": "^4.0.2",
    "lodash": "^4.17.4",
    "node-fetch": "^2.2.0",
    "pouchdb": "^7.0.0",
    "pouchdb-adapter-node-websql": "^7.0.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-hot-loader": "^4.3.4",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "babel-eslint": "^7",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "electron-forge": "^5.2.2",
    "electron-prebuilt-compile": "2.0.4",
    "eslint": "^4",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.10.0",
    "prettier": "^1.13.7"
  }
}
